<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Delicious Foods</title>
</head>
<body>
    <h1>Here are some delicious foods!!!!!</h1>
    @{
        var FoodList = new List<string>{
            "apple pie",
            "chicken tacos",
            "maple bar donuts",
            "my momma's fresh kimchi"
        };
        foreach (var food in FoodList){
            if (food[0].ToString() == "c"){
                <p style="color: red">@food</p>
            } else {
                <p>@food</p>
            }
        }
    }
</body>
</html>